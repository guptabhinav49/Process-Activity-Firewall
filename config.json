{
    "file_groups": [
        {
            "type" : 1,
            "exprs": [
                "^/dev/pts/*"
            ],
            "mode": 2
        },
        {
            "type" : 2,
            "exprs": [
                "((ba(na)*)+)",
                "(repos)+"
            ],
            "mode": 1
        }
    ],
    "ignore_groups": true,
    "verbose": true,
    "logfile_path": "../logs.json"
}